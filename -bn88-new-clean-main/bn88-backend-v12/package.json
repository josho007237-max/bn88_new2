{
  "name": "bn88-backend-v12",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev:prep": "prisma generate && prisma db push && npm run seed:dev",
    "dev:backend": "tsx watch src/server.ts",
    "dev:port": "node -e \"process.env.PORT='3001';require('child_process').spawn('tsx',['watch','src/server.ts'],{stdio:'inherit',env:process.env});\"",
    "dev:tunnel": "cloudflared tunnel run bn9-api",
    "dev:frontend": "npm --prefix ..\\bn88-frontend-dashboard-v12 run dev -- --port 5555",
    "dev:all": "run-p dev:backend dev:tunnel dev:frontend",
    "dev:full": "npm run dev:prep && npm run dev:all",
    "dev:check": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-check.ps1",
    "port:3000": "powershell -ExecutionPolicy Bypass -File ./scripts/port-3000.ps1",
    "port:3000:kill": "powershell -ExecutionPolicy Bypass -File ./scripts/port-3000.ps1 -Kill",
    "check:all": "pwsh -ExecutionPolicy Bypass -File ./src/scripts/dev-check.ps1",
    "check:line": "pwsh -ExecutionPolicy Bypass -File ./src/scripts/dev-check-line.ps1",
    "dev:check:chat": "tsx src/scripts/dev-check-chat.ts",
    "check:extended": "pwsh -ExecutionPolicy Bypass -File ./src/scripts/dev-check-extended.ps1",
    "seed": "tsx src/scripts/seed.ts",
    "seed:admin": "tsx src/scripts/seedAdmin.ts",
    "seed:bot": "tsx src/scripts/seedBot.ts",
    "seed:dev": "tsx src/scripts/seedDev.ts",
    "dev:seed:line": "tsx src/scripts/seedLineSecret.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:studio": "prisma studio",
    "migrate:deploy": "prisma migrate deploy",
    "predev": "npm run prisma:generate:safe",
    "dev": "tsx watch src/server.ts",
    "doctor": "node -v && npm -v && node -e \"const fs=require('fs');const p='node_modules/.bin/tsx';const PORT=process.env.PORT||'3000';const REDIS_URL=process.env.REDIS_URL||'';const REDIS_PORT=process.env.REDIS_PORT||'';const REDIS_HOST=process.env.REDIS_HOST||'';console.log(fs.existsSync(p)?'tsx OK: '+p:'tsx MISSING: '+p);console.log('PORT='+PORT);console.log('REDIS_URL='+REDIS_URL);console.log('REDIS_HOST='+REDIS_HOST);console.log('REDIS_PORT='+REDIS_PORT);\"",
    "postinstall": "prisma generate",
    "studio": "prisma studio --port 5556",
    "start": "node dist/server.js",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "check:env": "powershell -ExecutionPolicy Bypass -File ./scripts/check-env.ps1",
    "prisma:generate:safe": "prisma generate",
    "predev:backend": "npm run prisma:generate:safe",
    "predev:port": "npm run prisma:generate:safe"
  },
  "dependencies": {
    "@line/bot-sdk": "^10.5.0",
    "@prisma/client": "^6.19.2",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.29.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^6.8.1",
    "recharts": "^3.3.0",
    "redis": "^4.6.14",
    "sharp": "^0.34.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.27",
    "concurrently": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.19.2",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^1.6.0"
  }
}
